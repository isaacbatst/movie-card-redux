{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "trybe-frontend",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "sonarjs"],
  "rules": {
    "react/prop-types": 0,
    "no-underscore-dangle":["error", {
      "allow": ["__REDUX_DEVTOOLS_EXTENSION__"]
    }]
  }
}